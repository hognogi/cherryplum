{
  "name": "cheryplum",
  "version": "0.2.0",
  "description": "A full stack framwork optimised for multiple front-end interfaces. Demo interfaces built with React.js and DoT.js",
  "scripts": {
    "test": "mocha --require ignore-styles --require jsdomSetup.js --compilers js:babel-register ./src/**/*.spec.js --recursive",
    "test:watch": "npm test -- --watch",
    "clean": "rimraf dist",
    "build:webpack": "set NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node cherryplum.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hognogi/cherryplum"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "reload",
    "webpack",
    "babel",
    "scss",
    "TDD",
    "enzyme",
    "mocha",
    "sinom"
  ],
  "author": "Beni Hognogi",
  "license": "MIT",
  "homepage": "https://github.com/hognogi/cherryplum#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^5.0.0-beta2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "css-loader": "^0.23.0",
    "enzyme": "^2.5.1",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^2.3.0",
    "except": "^0.1.3",
    "file-loader": "^0.8.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^0.14.8",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "sass-mq": "^3.2.9",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "jwt-simple": "^0.5.1",
    "react": "^0.14.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^0.14.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "bluebird": "^3.0.5",
    "dateformat": "^1.0.12",
    "dot": "^1.0.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "extend": "^3.0.0",
    "multer": "^1.1.0",
